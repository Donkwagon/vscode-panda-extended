{
  "$schema": "vscode://schemas/color-theme",
  "comment": "Panda, Created by Siamak Mokhtari.",
  "author": "Siamak Mokhtari",
  "name": "Panda Theme Extended",
  "tokenColors": [
    {
      "settings": {
        "background": "$gray",
        "foreground": "$white"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "$comment-gray",
        "fontStyle": " italic"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "$green"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "$orange"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "$orange"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "$light-blue"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": "",
        "foreground": "$light-pink"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "$pink"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "$orange"
      }
    },
    {
      "name": "Storage type",
      "scope": "storage.type",
      "settings": {
        "foreground": "$orange"
      }
    },
    {
      "name": "Class name",
      "scope": "entity.name.class",
      "settings": {
        "foreground": "$light-orange",
        "fontStyle": " italic"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "fontStyle": "   underline ",
        "foreground": "$light-orange"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "fontStyle": "",
        "foreground": "$light-blue"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "$lighter-gray",
        "fontStyle": " italic"
      }
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "fontStyle": "",
        "foreground": "$red"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "",
        "foreground": "$orange"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "fontStyle": "",
        "foreground": "$light-blue"
      }
    },
    {
      "name": "Library constant",
      "scope": "support.constant",
      "settings": {
        "fontStyle": "",
        "foreground": "$orange"
      }
    },
    {
      "name": "Library class\/type",
      "scope": "support.type, support.class",
      "settings": {
        "foreground": "$light-orange"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {
        "fontStyle": "",
        "foreground": "$red"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "background": "",
        "foreground": "$lighter-gray"
      }
    },
    {
      "name": "Invalid deprecated",
      "scope": "invalid.deprecated",
      "settings": {
        "background": "",
        "foreground": "$light-gray",
        "fontStyle": "  italic underline"
      }
    },
    {
      "name": "Italic HTML attribute names",
      "scope": "\n\t\t\t\t, constant.other.symbol.ruby,\n\t\t\t\t, entity.other.attribute-name.jsx,\n\t\t\t",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "",
      "scope": "source meta.property-list meta.property-name support.type.property-name",
      "settings": {
        "foreground": "$very-light-gray"
      }
    },
    {
      "name": "",
      "scope": "source meta.property-list meta.property-value keyword.other.unit",
      "settings": {
        "foreground": "$orange"
      }
    },
    {
      "name": "source.js keyword.operator.js",
      "scope": "source keyword.operator",
      "settings": {
        "foreground": "$very-light-gray"
      }
    },
    {
      "name": "",
      "scope": "source.js entity.name.type.object.js.firebug",
      "settings": {
        "foreground": "$light-orange"
      }
    },
    {
      "name": "",
      "scope": "source meta.class.instance.constructor keyword.operator.new",
      "settings": {
        "fontStyle": " italic",
        "background": "",
        "foreground": "$pink"
      }
    },
    {
      "name": "",
      "scope": "source.js meta.class.instance.constructor entity.name.type.instance.js",
      "settings": {
        "foreground": "$light-orange"
      }
    },
    {
      "name": "",
      "scope": "source meta.at-rule.media",
      "settings": {
        "foreground": "$purple"
      }
    },
    {
      "name": "",
      "scope": "source meta.at-rule.import variable.parameter.url",
      "settings": {
        "foreground": "$green"
      }
    },
    {
      "name": "",
      "scope": "source.sass variable.parameter.sass",
      "settings": {
        "foreground": "$light-pink"
      }
    },
    {
      "name": "",
      "scope": "source meta.property-value.css constant.other",
      "settings": {
        "foreground": "$orange"
      }
    },
    {
      "name": "",
      "scope": "source .property-value.css parameter constant.numeric.css keyword.other.unit.css",
      "settings": {
        "foreground": "$orange"
      }
    }
  ],
  "colorSpaceName": "sRGB",
  "semanticClass": "theme.dark.sublime_panda_syntax"
}
